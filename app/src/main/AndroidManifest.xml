<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.csulb.cecsymemeteam.dinnr">

    <!--
         The ACCESS_COARSE/FINE_LOCATION permissions are not required to use
         Google Maps Android API v2, but you must specify either coarse or fine
         location permissions for the 'MyLocation' functionality. 
    -->
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        android:theme="@style/Theme.AppCompat.Light.NoActionBar" >
        <activity android:name=".FirstTimeStartupActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity" />
        <activity android:name=".NewAccountActivity" />
        <activity android:name=".LoginActivity" />
        <activity android:name=".ForgottenLoginActivity" />
        <activity android:name=".ProfileActivity" />
        <activity android:name=".MoreReviewsActivity" />
        <activity android:name=".WriteReviewActivity" />
        <activity android:name=".UserProfileActivity" />
        <activity android:name=".videoActivity"></activity>
    </application>

</manifest>